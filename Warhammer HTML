<script>
  // Get the header section element
  const headerSection = document.getElementById('headerSection');

  // Get all the navigation links by their respective classes
  const mainWebsiteLink = document.querySelector('.main-website-link');
  const orderOnlineLink = document.querySelector('.order-online-link');
  const loreLink = document.querySelector('.lore-link');
  const shopLink = document.querySelector('.shop-link');

  // Add event listeners to change the background image for each link
  mainWebsiteLink.addEventListener('mouseover', function() {
    headerSection.style.backgroundImage = 'url(Images/BackgroundImageOne.jpg)';
  });

  mainWebsiteLink.addEventListener('mouseout', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/BackgroundAttemptTwo.jpg)';
  });

  orderOnlineLink.addEventListener('mouseover', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/Walli.jpg)';
  });

  orderOnlineLink.addEventListener('mouseout', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/BackgroundAttemptTwo.jpg)';
  });

  loreLink.addEventListener('mouseover', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/32l.jpg)';
  });

  loreLink.addEventListener('mouseout', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/BackgroundAttemptTwo.jpg)';
  });

  shopLink.addEventListener('mouseover', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/shop.jpg)';
  });

  shopLink.addEventListener('mouseout', function() {
    headerSection.style.backgroundImage = 'linear-gradient(rgba(4, 9, 30, 0.4), rgba(4, 9, 30, 0.4)), url(Images/BackgroundAttemptTwo.jpg)';
  });
</script>


</ul>

